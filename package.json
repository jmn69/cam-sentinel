{
  "name": "cam-sentinel",
  "version": "1.0.0",
  "description": "Cam state monitoring",
  "author": "Jordane Michon",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
    "yarn": ">=1.5.1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --write",
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "scripts": {
    "precommit": "lint-staged",
    "start": "node index.js",
    "lint": "esw *.js config --color",
    "lint:watch": "yarn lint -- --watch"
  },
  "dependencies": {
    "aws-sdk": "^2.507.0",
    "axios": "^0.19.0",
    "bluebird": "3.5.5",
    "body-parser": "1.19.0",
    "dotenv": "^8.0.0",
    "http-status": "^1.3.2",
    "joi": "^14.3.1",
    "mongoose": "^5.7.5",
    "ssl-root-cas": "1.2.5",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-watch": "^6.0.0",
    "husky": "^2.4.1",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2"
  }
}
