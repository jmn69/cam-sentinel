{
  "name": "cam-sentinel",
  "version": "1.0.0",
  "description": "Cam state monitoring",
  "author": "Jordane Michon",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=5.6.0",
